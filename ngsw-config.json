{
    "$schema": "./node_modules/@angular/service-worker/config/schema.json",
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/*.css",
                "/*.js"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**",
                "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
            ]
        }
    }],
    "dataGroups": [{
        "name": "evangelizo-api",
        "urls": ["https://publication.evangelizo.ws/AM/days/*", "https://beta.ourmanna.com/api/v1/get/?format=json"],
        "cacheConfig": {
            "maxSize": 100,
            "maxAge": "1d",
            "strategy": "freshness"
        }
    }]
}